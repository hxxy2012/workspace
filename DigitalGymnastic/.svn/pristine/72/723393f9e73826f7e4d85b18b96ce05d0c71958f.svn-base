<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_bg_login"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/top_ly"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone" >
        <include layout="@layout/topbar" />
    </LinearLayout>

    <Button
        android:id="@+id/btn_next_two"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_margin="@dimen/x18"
        android:background="@drawable/btn_selector"
        android:gravity="center"
        android:text="@string/next"
        android:visibility="gone" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/top_ly"
        android:orientation="vertical" >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y21"
            android:gravity="center_horizontal"
            android:text="@string/selcetage"
            android:textSize="@dimen/x12"
            android:textColor="@color/umeng_socialize_text_share_content" />
        <TextView
            android:id="@+id/yeartext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y28"
            android:layout_marginBottom="@dimen/x15"
            android:gravity="center_horizontal"
            android:text="1990"
            android:textSize="@dimen/x20"
            android:textColor="@color/green_00bfa3" />
        <LinearLayout
            android:id="@+id/root"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/y225"
            android:orientation="vertical" >
            <com.hike.digitalgymnastic.view.YearVerticalRuler
                android:id="@+id/yearruler"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y225"
                />
    </LinearLayout>
    </LinearLayout>

</RelativeLayout><!-- From: file:/D:/techfit3_1/app/src/main/res/layout/personalinfo_two.xml -->