        android:layout_height="@dimen/x1"
        android:background="@color/grey_e5"
        />
    <RelativeLayout
        android:id="@+id/menu_set_rl_feedback"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y40">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x9"
            android:text="@string/menu_feedback"
            android:textColor="@color/umeng_socialize_list_item_textcolor"
            android:textSize="@dimen/x13" />

        <ImageView
            android:layout_width="@dimen/x7"
            android:layout_height="@dimen/x11"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/x14"
            android:background="@mipmap/icon_set_right_arrow" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/x1"
        android:background="@color/grey_e5"
        android:layout_marginLeft="@dimen/x25"
        />

    <RelativeLayout
        android:id="@+id/menu_set_rl_aboutme"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y40" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x9"
            android:text="关于我们"
            android:textColor="@color/umeng_socialize_list_item_textcolor"
            android:textSize="@dimen/x13" />

        <ImageView
            android:layout_width="@dimen/x7"
            android:layout_height="@dimen/x11"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/x14"
            android:background="@mipmap/icon_set_right_arrow" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/x1"
        android:background="@color/grey_e5"
        />

    <Button
        android:id="@+id/menu_set_bt_loginout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y40"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/x9"
        android:layout_marginRight="@dimen/x9"
        android:layout_marginTop="@dimen/y20"
        android:layout_marginBottom="@dimen/y20"
        android:text="@string/menu_login_out"
        android:textColor="@color/white"
        android:textSize="@dimen/x13"
        android:background="@color/text_green"
        />
