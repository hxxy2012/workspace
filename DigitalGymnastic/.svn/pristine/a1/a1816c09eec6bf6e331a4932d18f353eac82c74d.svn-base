package com.hike.digitalgymnastic.entitiy;

import java.io.Serializable;

//ShuifenData(身体水分数据):
public class ShuifenData implements Serializable{
	
	private	ShuifenStandard standard;//身体水分标准	
	private short grade;//身体水分评级(-1:偏低;0:标准;1:偏高)	
	private double value;//身体水分值(%)
	public ShuifenStandard getStandard() {
		return standard;
	}
	public void setStandard(ShuifenStandard standard) {
		this.standard = standard;
	}
	public short getGrade() {
		return grade;
	}
	public void setGrade(short grade) {
		this.grade = grade;
	}
	public double getValue() {
		return value;
	}
	public void setValue(double value) {
		this.value = value;
	}
	
}
