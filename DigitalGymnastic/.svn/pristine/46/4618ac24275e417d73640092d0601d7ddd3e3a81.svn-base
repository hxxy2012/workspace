package com.hike.digitalgymnastic.entitiy;

import java.io.Serializable;

//ZhifanglvData(身体脂肪率数据):
public class ZhifanglvData implements Serializable{
	private ZhifanglvStandard standard;//身体脂肪率标准
	private short grade;//身体脂肪率评级(-1:瘦;0:标准;1:轻度肥胖;2:肥胖)
	private double value;//身体脂肪率值(%)
	public ZhifanglvStandard getStandard() {
		return standard;
	}
	public void setStandard(ZhifanglvStandard standard) {
		this.standard = standard;
	}
	public short getGrade() {
		return grade;
	}
	public void setGrade(short grade) {
		this.grade = grade;
	}
	public double getValue() {
		return value;
	}
	public void setValue(double value) {
		this.value = value;
	}
	
}
