<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GuiderActivity" >

    <android.support.v4.view.ViewPager
        android:id="@+id/viewPager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fadingEdge="none" >

        <LinearLayout
            android:id="@+id/ll_videoView_one"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <com.hike.digitalgymnastic.view.CustomerVideoView
                android:id="@+id/videoView_one"
                android:layout_width="match_parent"
                android:layout_height="match_parent" 
                />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_videoView_two"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <com.hike.digitalgymnastic.view.CustomerVideoView
                android:id="@+id/videoView_two"
                android:layout_width="match_parent"
                android:layout_height="match_parent" 
                />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_videoView_three"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <com.hike.digitalgymnastic.view.CustomerVideoView
                android:id="@+id/videoView_three"
                android:layout_width="match_parent"
                android:layout_height="match_parent" 
                />
        </LinearLayout>
    </android.support.v4.view.ViewPager>

    <LinearLayout
        android:id="@+id/bottom_btns_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:gravity="center_horizontal"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/btn_register"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/y30"
            android:layout_marginBottom="@dimen/y7"
            android:layout_marginLeft="@dimen/x9"
            android:layout_marginRight="@dimen/x1"
            android:layout_marginTop="@dimen/y7"
            android:layout_weight="1"
            android:background="@drawable/btn_guider_register_selector"
            android:gravity="center"
            android:orientation="horizontal"
            android:text="@string/register"
            android:textColor="@color/white"
            android:textSize="@dimen/x16" />

        <Button
            android:id="@+id/btn_login"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/y30"
            android:layout_marginBottom="@dimen/y7"
            android:layout_marginLeft="@dimen/x1"
            android:layout_marginRight="@dimen/x9"
            android:layout_marginTop="@dimen/y7"
            android:layout_weight="1"
            android:background="@drawable/btn_guider_login_selector"
            android:gravity="center"
            android:orientation="horizontal"
            android:text="@string/login"
            android:textColor="@color/white"
            android:textSize="@dimen/x16" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/bottom_btns_layout"
        android:layout_centerHorizontal="true"
        android:gravity="center_horizontal"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/page0"
            android:layout_width="@dimen/x5"
            android:layout_height="@dimen/x5"
            android:layout_gravity="center_vertical"
            android:layout_margin="@dimen/x4"
            android:src="@mipmap/page_indicator_focused" />

        <ImageView
            android:id="@+id/page1"
             android:layout_width="@dimen/x5"
            android:layout_height="@dimen/x5"
            android:layout_gravity="center_vertical"
            android:layout_margin="@dimen/x4"
            android:src="@mipmap/page_indicator_unfocused" />

        <ImageView
            android:id="@+id/page2"
             android:layout_width="@dimen/x5"
            android:layout_height="@dimen/x5"
            android:layout_gravity="center_vertical"
            android:layout_margin="@dimen/x4"
            android:src="@mipmap/page_indicator_unfocused" />
    </LinearLayout>

</RelativeLayout><!-- From: file:/D:/techfit3_1/app/src/main/res/layout/activity_guider.xml -->