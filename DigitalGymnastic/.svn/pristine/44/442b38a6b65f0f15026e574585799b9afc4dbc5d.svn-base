<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_bg_login"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >
<!--
        <Button
            android:id="@+id/btn_next_five"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_margin="@dimen/x18"
            android:background="@drawable/btn_selector"
            android:gravity="center"
            android:text="@string/next"
            android:visibility="gone" />
 -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y23"
            android:minHeight="@dimen/y225"
            android:orientation="vertical" >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/x13"
                android:textColor="@color/umeng_socialize_text_share_content"
                android:text="@string/head_tip"
                android:gravity="center"
                android:layout_marginTop="@dimen/y20"/>
            <ImageView
                android:id="@+id/iv_camera"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_margin="@dimen/x18"
                android:src="@mipmap/boy_head" />

            <LinearLayout
                android:layout_width="@dimen/x220"
                android:layout_height="@dimen/x40"
                android:layout_marginTop="@dimen/x20"
                android:layout_gravity="center_horizontal"
                android:background="@color/app_bg_login"
                android:orientation="vertical" >
<!--
                <ImageView
                    android:layout_width="@dimen/x15"
                    android:layout_height="@dimen/x20"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/x5"
                    android:src="@mipmap/icon_touxiang" />
 -->
                <EditText
                    android:id="@+id/et_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/x5"
                    android:background="@color/app_bg_login"
                    android:gravity="center_vertical|center"
                    android:hint="@string/input_nick"
                    android:singleLine="true"
                    android:textColor="@color/umeng_socialize_text_share_content"
                    android:textSize="@dimen/x13" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x1"
                    android:layout_marginTop="@dimen/x10"
                    android:background="@color/grey_cc"/>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

</LinearLayout>