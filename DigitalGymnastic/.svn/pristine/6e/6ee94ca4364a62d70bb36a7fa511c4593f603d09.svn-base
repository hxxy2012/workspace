package com.hike.digitalgymnastic.tools;

import android.util.Log;

/**
 * Created by liqing on 16/3/5.
 * 类说明：当debug=true 打印日志 否则不打印
 * <p/>
 * 修改时间：16/3/5
 * 修改人：liqing
 * 修改内容：
 */
public class UtilLog {

    public  static boolean isDebug = true;

    private static String TAG = "techfit";

    public static void e(String pTag, String pMessage) {
        if (isDebug) {
            Log.e(TAG, pTag + "++++++++++  " + pMessage + "  ++++++++++");
        }
    }

}
