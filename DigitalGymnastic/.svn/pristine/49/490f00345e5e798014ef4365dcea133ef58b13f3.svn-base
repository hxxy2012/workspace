package com.hike.digitalgymnastic.entitiy;

import java.io.Serializable;

//BmiStandard(体质指数标准):
public class BmiStandard implements Serializable{
	private double normalFloor;//正常体质指数下限
//	private double mezzoFloor;//适中体质指数下限	
	private double heavierFloor;//过重体质指数下限	
	private double fatFloor;//肥胖体质指数下限	
	private double veryFatFloor;//非常胖体质指数下限
	public double getNormalFloor() {
		return normalFloor;
	}
	public void setNormalFloor(double normalFloor) {
		this.normalFloor = normalFloor;
	}
//	public double getMezzoFloor() {
//		return mezzoFloor;
//	}
//	public void setMezzoFloor(double mezzoFloor) {
//		this.mezzoFloor = mezzoFloor;
//	}
	public double getHeavierFloor() {
		return heavierFloor;
	}
	public void setHeavierFloor(double heavierFloor) {
		this.heavierFloor = heavierFloor;
	}
	public double getFatFloor() {
		return fatFloor;
	}
	public void setFatFloor(double fatFloor) {
		this.fatFloor = fatFloor;
	}
	public double getVeryFatFloor() {
		return veryFatFloor;
	}
	public void setVeryFatFloor(double veryFatFloor) {
		this.veryFatFloor = veryFatFloor;
	}

}
